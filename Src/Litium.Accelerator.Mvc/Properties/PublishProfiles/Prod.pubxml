<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>tengtools.preflight.workplace.nu</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>https://LIT-OD04.litiumdrift.se:8172/msdeploy.axd</MSDeployServiceURL>
    <DeployIisAppPath>Tengtools-Prod</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>LITIUMDRIFT\andreas.crona</UserName>
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ElasticsearchConnectionString" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="http://localhost:9200" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\ElasticsearchConnectionString_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="http://localhost:9200" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="FoundationConnectionString" Order="2" Enabled="False">
          <Destination Path="Data Source=LIT-SQL02;Initial Catalog=Tengtools-Prod-Rev1;Integrated Security=True;Pooling=True" Name="Pooling=true;Database=Tengtools-Prod-Rev1;Server=LIT-SQL02;Integrated security=true;MultipleActiveResultSets=True" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=localhost;Initial Catalog=tengtools;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\FoundationConnectionString_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Server=localhost; Initial Catalog=tengtools; Integrated Security=True; MultipleActiveResultSets=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)ElasticsearchConnectionString-Web.config Connection String" />
    <MSDeployParameterValue Include="FoundationConnectionString-Web.config Connection String">
      <ParameterValue>Pooling=true;Database=Tengtools-Prod-Rev1;Server=LIT-SQL02;Integrated security=true;MultipleActiveResultSets=True</ParameterValue>
    </MSDeployParameterValue>
